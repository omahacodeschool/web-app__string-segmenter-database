<center><h1> String Segmenter Useage History</h1></center>
<table border="1">
  <thead>
    <tr>
      <th>ID</th>
      <th>Original String</th>
      <th>Result</th>
      <th>Valid</th>
      <th>Timestamp</th>
    </tr>
  </thead>

  <tbody>
    <% @history.each do |a| %>
      <tr>
        <td><%= a.id%></td>
        <td><%= a.original_string %></td>
        <td><%=a.result%></td>
        <td>beep</td>
        <td><%=a.created_at%></td>
      </tr>
    <% end %>
  </tbody>
</table>